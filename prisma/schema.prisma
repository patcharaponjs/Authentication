// prisma/schema.prisma
generator client {
 provider = "prisma-client-js"
}
datasource db {
 provider = "postgresql"
 url = env("DATABASE_URL")
}
model Product  {
 id Int @id @default(autoincrement())
 product_name String
 product_type String
 price Int
 unit String
 createdAt DateTime @default(now())
}
model User  {
 id Int @id @default(autoincrement())
 user_name String
 password String
 name String
 role String
 createdAt DateTime @default(now())
}

